<!DOCTYPE web-app PUBLIC
        "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
        "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
    <display-name>Archetype Created Web Application</display-name>
    <servlet>
        <servlet-name>Pubs</servlet-name>
        <servlet-class>servlet.PubsServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Pub</servlet-name>
        <servlet-class>servlet.PubServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Students</servlet-name>
        <servlet-class>servlet.StudentsServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>StudentNodes</servlet-name>
        <servlet-class>servlet.StudentNodesServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>StudentsNode</servlet-name>
        <servlet-class>servlet.StudentsNodeServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Pubs</servlet-name>
        <url-pattern>/monitor/pubs</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Pub</servlet-name>
        <url-pattern>/monitor/general/node</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Students</servlet-name>
        <url-pattern>/monitor/students</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>StudentNodes</servlet-name>
        <url-pattern>/monitor/student/nodes</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>StudentsNode</servlet-name>
        <url-pattern>/monitor/students/node</url-pattern>
    </servlet-mapping>
    <error-page>
        <error-code>404</error-code>
        <location>/404.html</location>
    </error-page>
</web-app>
