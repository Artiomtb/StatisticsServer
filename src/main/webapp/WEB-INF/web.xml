<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
    <servlet>
        <servlet-name>Nodes</servlet-name>
        <servlet-class>servlet.NodesServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Pub</servlet-name>
        <servlet-class>servlet.PubServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Students</servlet-name>
        <servlet-class>servlet.StudentsServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Nodes</servlet-name>
        <url-pattern>/monitor/nodes</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Pub</servlet-name>
        <url-pattern>/monitor/general/node</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Students</servlet-name>
        <url-pattern>/monitor/students</url-pattern>
    </servlet-mapping>
</web-app>
